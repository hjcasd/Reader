<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--标题栏-->
    <declare-styleable name="TitleBar">
        <attr name="leftImage" format="integer" />

        <attr name="titleText" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleColor" format="color" />

        <attr name="rightImage" format="integer" />

        <attr name="isShowLine" format="boolean" />
    </declare-styleable>

    <!--流式布局标签-->
    <declare-styleable name="SmartFlowLayout">
        <attr name="maxLines" format="integer" />
        <attr name="childHorizontalMargin" format="dimension" />
        <attr name="childVerticalMargin" format="dimension" />
        <attr name="gravity">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

</resources>